{
  "name": "kurs-pm-web",
  "version": "1.0.0",
  "description": "Kurs-pm-data-web is a microservice with the public view of course memos. It uses React, MobX, and is based on KTH/node-web.",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/kurs-pm-web"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prepare": "husky install",
    "cy:open": "cypress open -C test/e2e/cypress.env.json",
    "cy:run": "cypress run -C test/e2e/cypress.env.json",
    "cy:run:chrome": "cypress run -C test/e2e/cypress.env.json --browser chrome",
    "cy:run:ff": "cypress run -C test/e2e/cypress.env.json --browser firefox",
    "cy:run:edge": "cypress run -C test/e2e/cypress.env.json --browser edge",
    "test": "NODE_ENV=test jest --testPathIgnorePatterns=test/e2e",
    "test-win": "cross-env NODE_ENV=test jest --testPathIgnorePatterns=test/e2e",
    "test:watch": "NODE_ENV=test jest --watch --testPathIgnorePatterns=test/e2e",
    "test:mockapi": "docker-compose -f test/mock-api/docker-compose.yml up --build --force-recreate",
    "test-unit-in-docker": "ID=$(docker build -q .) && LOCAL_IMAGE_ID=$ID docker-compose -f docker-compose-unit-tests.yml up --build --abort-on-container-exit --always-recreate-deps --force-recreate",
    "test-integration-in-docker": "ID=$(docker build -q .) && LOCAL_IMAGE_ID=$ID docker-compose -f docker-compose-integration-tests.yml up --build --abort-on-container-exit --always-recreate-deps --force-recreate",
    "test-performance-in-docker": "ID=$(docker build -q .) && LOCAL_IMAGE_ID=$ID docker-compose -f docker-compose-performance-tests.yml up --build --abort-on-container-exit --remove-orphans",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production rm -rf dist && parcel build ./public/js/app/app.jsx --public-url /kurs-pm/static",
    "build-dev": "cross-env NODE_ENV=development rm -rf dist && parcel watch ./public/js/app/app.jsx --public-url /kurs-pm/static",
    "build-dev-win": "cross-env NODE_ENV=development rimraf dist && parcel watch ./public/js/app/app.jsx --public-url /kurs-pm/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\"",
    "start-dev-win": "cross-env NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev-win\" \"nodemon --ignore dist/ --ignore public/ app.js\""
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@kth/kth-kip-style-react-components": "^0.1.3",
    "applicationinsights": "^2.1.3",
    "axios": "^0.21.2",
    "body-parser": "^1.19.0",
    "component-registry": "^2.0.0",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.4",
    "dotenv": "^10.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "http-proxy-middleware": "^2.0.1",
    "kth-node-access-log": "^0.2.4",
    "kth-node-api-call": "^3.0.16",
    "kth-node-configuration": "^2.0.5",
    "kth-node-express-routing": "^2.0.9",
    "kth-node-i18n": "^1.0.10",
    "kth-node-log": "^3.1.7",
    "kth-node-monitor": "^1.1.12",
    "kth-node-redis": "^3.1.30",
    "kth-node-server": "^3.1.5",
    "kth-node-session": "^2.0.2",
    "kth-node-web-common": "^5.1.16",
    "kth-style": "^6.0.21",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^4.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@kth/eslint-config-kth": "^1.0.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^7.7.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^16.14.0",
    "sass": "^1.35.2"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": "14.0.0"
  }
}
