<!DOCTYPE html>
<html lang="{{lang}}">

<head>
  <title>KTH | {{aboutCourse.siteName}}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="{{description}}">
  <link rel="shortcut icon" id="favicon" href="{{proxyPrefix}}/static/icon/favicon">

  {{#if canonicalUrl}}
    <link href="{{canonicalUrl}}" rel="canonical">
  {{/if}}

  {{prefixStyle '/static/kth-style/css/kth-bootstrap.css' 'styles' 'all'}}
  {{prefixStyle '/assets/fonts.css' 'styles' 'all'}}
  {{prefixStyle '/static/app.css' 'styles' 'all'}}

  {{{render 'styles'}}}

  {{#if blocks.klaroConfig}}
    {{{blocks.klaroConfig}}}
    {{prefixScript '/static/kth-style/js/klaro-no-css.js' 'head-scripts'}}
  {{/if}}

  {{prefixScript '/static/browserConfig' 'head-scripts'}}
  {{prefixScript '/static/kth-style/js/backtotop.js' 'head-scripts'}}

  <script src="{{toolbarUrl}}"></script>
  
  {{{render 'head-scripts'}}}
  
    {{#if blocks.matomoAnalytics}}
    <!-- analytics start -->
      {{{blocks.matomoAnalytics}}}
    <!-- analytics end -->
  {{/if}}
</head>

<body class="p-print-5">
  <a class="skipToMainContent" href="#mainContent" tabindex="1">{{i18n 'skip_to_main_content' lang}}</a>
  
  {{> kthHeader }}
  <div class="kth-content">
    <div class="row">
      {{{body}}}
    </div>
  </div>
  {{> kthFooter }}
  {{{render 'scripts'}}}  
</body>

</html>