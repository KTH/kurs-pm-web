    {{#if blocks.matomoAnalytics}}
    <!-- analytics start -->
      {{{blocks.matomoAnalytics}}}
    <!-- analytics end -->
  {{/if}}

  <script type="module">
    import { MenuPanel } from '{{proxyPrefix}}/assets/js/index.js'
    MenuPanel.init(document.querySelector('.kth-header'), document.querySelectorAll('.kth-mega-menu .kth-menu-item'))
    MenuPanel.init(document.querySelector('.kth-header'), document.querySelectorAll('.kth-menu-item.search'))
    MenuPanel.initModal(
      document.querySelector('.kth-menu-item.collapsable'),
      document.querySelector('.kth-menu-panel--modal')
    )
  </script>
</head>

<body class="p-print-5">
  {{#if lang}}
    <a class="skipToMainContent" href="#mainContent" tabindex="0">{{i18n 'skip_to_main_content' lang}}</a>
  {{else}}
    <a class="skipToMainContent" href="#mainContent" tabindex="0">{{i18n 'skip_to_main_content' 'en'}}</a>
  {{/if}}
  
  {{> kthHeader }}
  <div class="kth-content">
    <div class="row">
        {{{body}}}
    </div>
  </div>
  {{> kthFooter }}
  {{{render 'scripts'}}}
    <script type="text/javascript">
    // @page in CSS causes style override issues
    (function () {
      var m = window.matchMedia('print')
      function h(e) {
        if (e.matches) {
          document.body.style = 'margin-top: 0 !important;'
        } else {
          document.body.style = ''
        }
      }
      m.addListener(h)
      h(m)
    })();
  </script>
  
</body>

</html>