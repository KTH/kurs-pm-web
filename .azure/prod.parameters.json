{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "kurs-pm-web"
    },
    "env": {
      "value": "prod"
    },
    "secretNames": {
      "value": [
        "APPLICATIONINSIGHTS_CONNECTION_STRING",
        "KURSPLAN_API_KEY",
        "KURSPLAN_API_URI",
        "KURS_PM_DATA_API_KEY",
        "REDIS_URI",
        "SESSION_KEY",
        "SESSION_SECRET"
      ]
    },
    "environmentVariables": {
      "value": {
        "CAS_SSO_URI": "https://login.kth.se",
        "CM_HOST_URL": "https://www.kth.se/cm/",
        "CONSOLE_ENABLED": "false",
        "KOPPS_URI": "https://api.kth.se/api/kopps/v2/?defaultTimeout=60000",
        "KURS_PM_DATA_API_URI": "https://api.kth.se/api/kurs-pm-data?defaultTimeout=10000",
        "LOGGING_ACCESS_LOG": "true",
        "LOGGING_LEVEL": "INFO",
        "MEMO_STORAGE_URI": "https://kursinfostorageprod.blob.core.windows.net/memo-blob-container/",
        "NODE_ENV": "production",
        "SERVER_HOST_URL": "https://www.kth.se",
        "SERVER_PORT": "3000",
        "SERVICE_PUBLISH": "/kurs-pm",
        "SESSION_USE_REDIS": "true",
        "STDOUT_ENABLED": "true",
        "TOOLBAR_URL": "https://www.kth.se/social/toolbar/widget.js"
      }
    },
    "monitorPath": {
      "value": "/kurs-pm/_monitor"
    },
    "websitePort": {
      "value": "3000"
    }
  }
}